model Tariff {
  id            String   @id @default(uuid())
  code          String   @unique
  months        Int
  priceRUB      Int      @map("price_rub")
  priceStars    Int      @map("price_stars")
  priceOldRUB   Int?     @map("price_old_rub")
  priceOldStars Int?     @map("price_old_stars")
  discount      Int?
  perMonth      Int      @map("per_month")
  createdAt     DateTime @default(now()) @map("created_at")
  updatedAt     DateTime @updatedAt @map("updated_at")

  @@map("tariff")
}
